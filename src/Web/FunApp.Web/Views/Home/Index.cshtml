@model FunApp.Services.Models.Home.IndexViewModel
@{
    ViewData["Title"] = "Home Page";
}
<div class="row">
   
    @foreach (var joke in Model.Jokes)
    {
        <div class="col-md-12">
         <div class="panel panel-default">
             <div class="panel-body">
                 @joke.Content
             </div>
             <div class="panel-footer">@joke.CategoryName</div>
         </div>
        </div>
    }
</div>
